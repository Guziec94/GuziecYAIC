<TabItem x:Class="GuziecYAIC.KartaTworzeniaRozmowy"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GuziecYAIC"
      mc:Ignorable="d" 
      d:DesignHeight="370" d:DesignWidth="590">
    <Grid>
        <Label Content="Podaj adres IP rozmówcy" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtAdresIP" MaxLength="15" HorizontalAlignment="Left" Height="26" Margin="157,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnRozpocznijRozmowe" Content="Rozpocznij rozmowę" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Click="btnRozpocznijRozmowe_Click"/>
    </Grid>
</TabItem>
