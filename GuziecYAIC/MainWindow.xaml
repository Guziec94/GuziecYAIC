<Window x:Class="GuziecYAIC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GuziecYAIC"
        mc:Ignorable="d"
        Title="GuziecYAIC - Yet Another Internet Communicator" Width="600" Height="425" ResizeMode="CanMinimize">

        <Grid>
            <Frame x:Name="frameZawartosc" NavigationUIVisibility="Hidden" DockPanel.Dock="Top"/>
            <StatusBar VerticalAlignment="Bottom" Height="25">
                <Label x:Name="txtStatusBar" Content="txtStatusBar" VerticalAlignment="Center" Margin="5,0,0,0" Padding="0"/>
            </StatusBar>
        </Grid>
</Window>
